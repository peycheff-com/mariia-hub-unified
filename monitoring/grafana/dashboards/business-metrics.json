          "metricColumn": "booking_count",
          "rawQuery": true,
          "rawSql": "SELECT\n  s.name as service,\n  COUNT(b.id) as booking_count\nFROM bookings b\nJOIN services s ON b.service_id = s.id\nWHERE\n  $__timeFilter(b.created_at)\n  AND b.status = 'completed'\nGROUP BY s.name\nORDER BY booking_count DESC\nLIMIT 10",
