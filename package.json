{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:analyze": "npm run build && node scripts/analyze-bundle.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "dev:debug": "vite --debug",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:shard": "vitest run --shard=$VITEST_SHARD --shard-count=$VITEST_SHARD_COUNT",
    "test:ci-parallel": "concurrently \"VITEST_SHARD=0 VITEST_SHARD_COUNT=4 npm run test:shard\" \"VITEST_SHARD=1 VITEST_SHARD_COUNT=4 npm run test:shard\" \"VITEST_SHARD=2 VITEST_SHARD_COUNT=4 npm run test:shard\" \"VITEST_SHARD=3 VITEST_SHARD_COUNT=4 npm run test:shard\" --max-processes 4",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:codegen": "playwright codegen",
    "test:e2e:visual": "playwright test --grep \"visual\"",
    "test:e2e:ci": "playwright test --reporter=json --outputFile=test-results/e2e-results.json",
    "test:all": "npm run test && npm run test:e2e",
    "test:helpers": "node scripts/test-helpers.js",
    "test:generate": "node scripts/test-helpers.js generate",
    "test:clean": "node scripts/test-helpers.js clean",
    "test:health": "node scripts/test-helpers.js health",
    "test:analyze": "node scripts/test-helpers.js coverage",
    "test:focus": "node scripts/test-helpers.js run",
    "test:affected": "vitest run --run --reporter=verbose --passWithNoTests",
    "preview": "vite preview",
    "security-audit": "npm audit && node scripts/security-audit.cjs",
    "optimize-images": "node scripts/optimize-images.cjs",
    "optimize-images:webp": "node scripts/optimize-images-webp.js",
    "optimize-images:all": "npm run optimize-images && npm run optimize-images:webp",
    "optimize:assets": "node scripts/optimize-assets.js",
    "optimize:all": "npm run optimize:assets && npm run optimize-images",
    "check-deps": "npm outdated",
    "update-deps": "node scripts/update-dependencies.cjs",
    "db:seed:staging": "tsx scripts/seed-staging-data.ts",
    "db:seed:preview": "tsx scripts/seed-preview-data.ts",
    "db:reset:staging": "npx supabase db reset --db-url $STAGING_SUPABASE_URL",
    "setup:staging": "chmod +x scripts/setup-supabase-staging.sh && ./scripts/setup-supabase-staging.sh",
    "setup:domain": "chmod +x scripts/setup-staging-domain.sh && ./scripts/setup-staging-domain.sh",
    "health:staging": "./scripts/health-check.sh staging.mariaborysevych.com",
    "deploy:staging": "vercel --scope $VERCEL_ORG --confirm",
    "prepare": "husky install"
  },
