{
  "name": "@mariia-hub/api-client",
  "version": "1.0.0",
  "description": "Comprehensive TypeScript/JavaScript SDK for the Mariia Hub beauty and fitness booking platform",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "docs": "typedoc src/index.ts",
    "prepublishOnly": "npm run build && npm run test && npm run docs",
    "clean": "rimraf dist coverage"
  },
  "keywords": [
    "mariia-hub",
    "beauty",
    "fitness",
    "booking",
    "api",
    "sdk",
    "typescript",
    "javascript",
    "client",
    "wellness",
    "spa",
    "appointment"
  ],
  "author": {
    "name": "Mariia Hub Team",
    "email": "api@mariia-hub.com",
    "url": "https://mariia-hub.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mariia-hub/sdk.git",
    "directory": "typescript"
  },
  "bugs": {
    "url": "https://github.com/mariia-hub/sdk/issues"
  },
  "homepage": "https://docs.mariia-hub.com/sdk/typescript",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "ws": "^8.14.2",
    "axios": "^1.6.0",
    "jose": "^5.1.3",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/ws": "^8.5.8",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.4.0",
    "@rollup/plugin-typescript": "^11.1.5",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-json": "^6.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}